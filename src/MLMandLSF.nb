Cell[CellGroupData[{

Cell["Least Squares Fitting (LSF)", "Section",
 CellChangeTimes->{{3.92365364254618*^9, 
  3.923653647405414*^9}},ExpressionUUID->"641e5a9d-f74f-4081-aee9-\
c00b6be56c26"],

Cell[TextData[{
 "Least squares fitting is a method for finding a linear fit from a given set \
of data. This fit depends on summations of the x data, y data, x data \
squared, and x times y data. On top of that, if each measurement has a \
different uncertainty, then that can be taken into account as well. \n\nThe \
form of the linear fit for the data is: ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         StyleBox["y", "TI"], "\[LongEqual]", "\[Alpha]", "+", "\[Beta]", 
         StyleBox["x", "TI"]}], TraditionalForm], "errors" -> {}, "input" -> 
     "y=\\alpha + \\beta x", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "95a010b5-3fe2-4b4b-b654-6390b6c65163"],
 ", where:\n",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{"\[Alpha]", "\[LongEqual]", 
         FractionBox[
          RowBox[{
            UnderoverscriptBox["\[Sum]", 
             RowBox[{
               StyleBox["i", "TI"], "\[LongEqual]", "1"}], 
             StyleBox["n", "TI"], LimitsPositioning -> False], 
            FractionBox[
             SubscriptBox[
              StyleBox["y", "TI"], 
              StyleBox["i", "TI"]], 
             SubsuperscriptBox["\[Sigma]", 
              StyleBox["i", "TI"], "2"]], 
            UnderoverscriptBox["\[Sum]", 
             RowBox[{
               StyleBox["i", "TI"], "\[LongEqual]", "1"}], 
             StyleBox["n", "TI"], LimitsPositioning -> False], 
            FractionBox[
             SubsuperscriptBox[
              StyleBox["x", "TI"], 
              StyleBox["i", "TI"], "2"], 
             SubsuperscriptBox["\[Sigma]", 
              StyleBox["i", "TI"], "2"]], "-", 
            UnderoverscriptBox["\[Sum]", 
             RowBox[{
               StyleBox["i", "TI"], "\[LongEqual]", "1"}], 
             StyleBox["n", "TI"], LimitsPositioning -> False], 
            FractionBox[
             RowBox[{
               SubscriptBox[
                StyleBox["x", "TI"], 
                StyleBox["i", "TI"]], 
               SubscriptBox[
                StyleBox["y", "TI"], 
                StyleBox["i", "TI"]]}], 
             SubsuperscriptBox["\[Sigma]", 
              StyleBox["i", "TI"], "2"]], 
            UnderoverscriptBox["\[Sum]", 
             RowBox[{
               StyleBox["i", "TI"], "\[LongEqual]", "1"}], 
             StyleBox["n", "TI"], LimitsPositioning -> False], 
            FractionBox[
             SubscriptBox[
              StyleBox["x", "TI"], 
              StyleBox["i", "TI"]], 
             SubsuperscriptBox["\[Sigma]", 
              StyleBox["i", "TI"], "2"]]}], 
          RowBox[{
            UnderoverscriptBox["\[Sum]", 
             RowBox[{
               StyleBox["i", "TI"], "\[LongEqual]", "1"}], 
             StyleBox["n", "TI"], LimitsPositioning -> False], 
            FractionBox["1", 
             SubsuperscriptBox["\[Sigma]", 
              StyleBox["i", "TI"], "2"]], 
            UnderoverscriptBox["\[Sum]", 
             RowBox[{
               StyleBox["i", "TI"], "\[LongEqual]", "1"}], 
             StyleBox["n", "TI"], LimitsPositioning -> False], 
            FractionBox[
             SubsuperscriptBox[
              StyleBox["x", "TI"], 
              StyleBox["i", "TI"], "2"], 
             SubsuperscriptBox["\[Sigma]", 
              StyleBox["i", "TI"], "2"]], "-", 
            SuperscriptBox[
             RowBox[{"(", 
               RowBox[{
                 UnderoverscriptBox["\[Sum]", 
                  RowBox[{
                    StyleBox["i", "TI"], "\[LongEqual]", "1"}], 
                  StyleBox["n", "TI"], LimitsPositioning -> False], 
                 FractionBox[
                  SubscriptBox[
                   StyleBox["x", "TI"], 
                   StyleBox["i", "TI"]], 
                  SubsuperscriptBox["\[Sigma]", 
                   StyleBox["i", "TI"], "2"]]}], ")"}], "2"]}]]}], 
       TraditionalForm], "errors" -> {}, "input" -> 
     "\\alpha = \\frac{\\sum\\limits_{i=1}^{n} \\frac{y_i}{\\sigma_i^2} \\sum\
\\limits_{i=1}^{n} \\frac{x^2_i}{\\sigma_i^2} - \\sum\\limits_{i=1}^{n} \
\\frac{x_i y_i}{\\sigma_i^2} \\sum\\limits_{i=1}^{n} \
\\frac{x_i}{\\sigma_i^2}}{\\sum\\limits_{i=1}^{n} \\frac{1}{\\sigma_i^2} \
\\sum\\limits_{i=1}^{n} \\frac{x_i^2}{\\sigma_i^2} - \
\\left(\\sum\\limits_{i=1}^{n} \\frac{x_i}{\\sigma_i^2}\\right)^2}", "state" -> 
     "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "7d634a32-420f-4e27-afdd-9c4712270200"],
 "\n",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{"\[Beta]", "\[LongEqual]", 
         FractionBox[
          RowBox[{
            UnderoverscriptBox["\[Sum]", 
             RowBox[{
               StyleBox["i", "TI"], "\[LongEqual]", "1"}], 
             StyleBox["n", "TI"], LimitsPositioning -> False], 
            FractionBox["1", 
             SubsuperscriptBox["\[Sigma]", 
              StyleBox["i", "TI"], "2"]], 
            UnderoverscriptBox["\[Sum]", 
             RowBox[{
               StyleBox["i", "TI"], "\[LongEqual]", "1"}], 
             StyleBox["n", "TI"], LimitsPositioning -> False], 
            FractionBox[
             RowBox[{
               SubscriptBox[
                StyleBox["x", "TI"], 
                StyleBox["i", "TI"]], 
               SubscriptBox[
                StyleBox["y", "TI"], 
                StyleBox["i", "TI"]]}], 
             SubsuperscriptBox["\[Sigma]", 
              StyleBox["i", "TI"], "2"]], "-", 
            UnderoverscriptBox["\[Sum]", 
             RowBox[{
               StyleBox["i", "TI"], "\[LongEqual]", "1"}], 
             StyleBox["n", "TI"], LimitsPositioning -> False], 
            FractionBox[
             SubscriptBox[
              StyleBox["x", "TI"], 
              StyleBox["i", "TI"]], 
             SubsuperscriptBox["\[Sigma]", 
              StyleBox["i", "TI"], "2"]], 
            UnderoverscriptBox["\[Sum]", 
             RowBox[{
               StyleBox["i", "TI"], "\[LongEqual]", "1"}], 
             StyleBox["n", "TI"], LimitsPositioning -> False], 
            FractionBox[
             SubscriptBox[
              StyleBox["y", "TI"], 
              StyleBox["i", "TI"]], 
             SubsuperscriptBox["\[Sigma]", 
              StyleBox["i", "TI"], "2"]]}], 
          RowBox[{
            UnderoverscriptBox["\[Sum]", 
             RowBox[{
               StyleBox["i", "TI"], "\[LongEqual]", "1"}], 
             StyleBox["n", "TI"], LimitsPositioning -> False], 
            FractionBox["1", 
             SubsuperscriptBox["\[Sigma]", 
              StyleBox["i", "TI"], "2"]], 
            UnderoverscriptBox["\[Sum]", 
             RowBox[{
               StyleBox["i", "TI"], "\[LongEqual]", "1"}], 
             StyleBox["n", "TI"], LimitsPositioning -> False], 
            FractionBox[
             SubsuperscriptBox[
              StyleBox["x", "TI"], 
              StyleBox["i", "TI"], "2"], 
             SubsuperscriptBox["\[Sigma]", 
              StyleBox["i", "TI"], "2"]], "-", 
            SuperscriptBox[
             RowBox[{"(", 
               RowBox[{
                 UnderoverscriptBox["\[Sum]", 
                  RowBox[{
                    StyleBox["i", "TI"], "\[LongEqual]", "1"}], 
                  StyleBox["n", "TI"], LimitsPositioning -> False], 
                 FractionBox[
                  SubscriptBox[
                   StyleBox["x", "TI"], 
                   StyleBox["i", "TI"]], 
                  SubsuperscriptBox["\[Sigma]", 
                   StyleBox["i", "TI"], "2"]]}], ")"}], "2"]}]]}], 
       TraditionalForm], "errors" -> {}, "input" -> 
     "\\beta = \\frac{\\sum\\limits_{i=1}^{n} \\frac{1}{\\sigma_i^2} \
\\sum\\limits_{i=1}^{n} \\frac{x_i y_i}{\\sigma_i^2} - \
\\sum\\limits_{i=1}^{n} \\frac{x_i}{\\sigma_i^2} \\sum\\limits_{i=1}^{n} \
\\frac{y_i}{\\sigma_i^2}}{\\sum\\limits_{i=1}^{n} \\frac{1}{\\sigma_i^2} \
\\sum\\limits_{i=1}^{n} \\frac{x_i^2}{\\sigma_i^2} - \
\\left(\\sum\\limits_{i=1}^{n} \\frac{x_i}{\\sigma_i^2}\\right)^2}", "state" -> 
     "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "e7c9b3b1-044e-4761-a68c-f16a0769a492"],
 "\n\nIf all the uncertainties, ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    TemplateBox[<|"boxes" -> FormBox[
        SubscriptBox["\[Sigma]", 
         StyleBox["i", "TI"]], TraditionalForm], "errors" -> {}, "input" -> 
      "\\sigma_i", "state" -> "Boxes"|>,
     "TeXAssistantTemplate"], ","}], TraditionalForm]],ExpressionUUID->
  "d04cbe73-1c02-4dc9-9ae1-a04b5caee991"],
 " are the same, then the forms for these simplify down to:\n",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{"\[Alpha]", "\[LongEqual]", 
         FractionBox[
          RowBox[{
            UnderoverscriptBox["\[Sum]", 
             RowBox[{
               StyleBox["i", "TI"], "\[LongEqual]", "1"}], 
             StyleBox["n", "TI"], LimitsPositioning -> False], 
            SubsuperscriptBox[
             StyleBox["x", "TI"], 
             StyleBox["i", "TI"], "2"], 
            UnderoverscriptBox["\[Sum]", 
             RowBox[{
               StyleBox["i", "TI"], "\[LongEqual]", "1"}], 
             StyleBox["n", "TI"], LimitsPositioning -> False], 
            SubscriptBox[
             StyleBox["y", "TI"], 
             StyleBox["i", "TI"]], "-", 
            UnderoverscriptBox["\[Sum]", 
             RowBox[{
               StyleBox["i", "TI"], "\[LongEqual]", "1"}], 
             StyleBox["n", "TI"], LimitsPositioning -> False], 
            SubscriptBox[
             StyleBox["x", "TI"], 
             StyleBox["i", "TI"]], 
            UnderoverscriptBox["\[Sum]", 
             RowBox[{
               StyleBox["i", "TI"], "\[LongEqual]", "1"}], 
             StyleBox["n", "TI"], LimitsPositioning -> False], 
            SubscriptBox[
             StyleBox["x", "TI"], 
             StyleBox["i", "TI"]], 
            SubscriptBox[
             StyleBox["y", "TI"], 
             StyleBox["i", "TI"]]}], 
          RowBox[{
            StyleBox["n", "TI"], 
            UnderoverscriptBox["\[Sum]", 
             RowBox[{
               StyleBox["i", "TI"], "\[LongEqual]", "1"}], 
             StyleBox["n", "TI"], LimitsPositioning -> False], 
            SubsuperscriptBox[
             StyleBox["x", "TI"], 
             StyleBox["i", "TI"], "2"], "-", 
            SuperscriptBox[
             RowBox[{"(", 
               RowBox[{
                 UnderoverscriptBox["\[Sum]", 
                  RowBox[{
                    StyleBox["i", "TI"], "\[LongEqual]", "1"}], 
                  StyleBox["n", "TI"], LimitsPositioning -> False], 
                 SubscriptBox[
                  StyleBox["x", "TI"], 
                  StyleBox["i", "TI"]]}], ")"}], "2"]}]]}], TraditionalForm], 
     "errors" -> {}, "input" -> 
     "\\alpha = \\frac{\\sum\\limits_{i=1}^{n} x_i^2 \\sum\\limits_{i=1}^{n} \
y_i - \\sum\\limits_{i=1}^{n} x_i \\sum\\limits_{i=1}^{n} x_i y_i}{n \
\\sum\\limits_{i=1}^{n} x_i^2 - \\left(\\sum\\limits_{i=1}^{n} \
x_i\\right)^2}", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "cd92f6c7-0b66-457e-951b-e50d84df786e"],
 "\n",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{"\[Beta]", "\[LongEqual]", 
         FractionBox[
          RowBox[{
            StyleBox["n", "TI"], 
            UnderoverscriptBox["\[Sum]", 
             RowBox[{
               StyleBox["i", "TI"], "\[LongEqual]", "1"}], 
             StyleBox["n", "TI"], LimitsPositioning -> False], 
            SubscriptBox[
             StyleBox["x", "TI"], 
             StyleBox["i", "TI"]], 
            SubscriptBox[
             StyleBox["y", "TI"], 
             StyleBox["i", "TI"]], "-", 
            UnderoverscriptBox["\[Sum]", 
             RowBox[{
               StyleBox["i", "TI"], "\[LongEqual]", "1"}], 
             StyleBox["n", "TI"], LimitsPositioning -> False], 
            SubscriptBox[
             StyleBox["x", "TI"], 
             StyleBox["i", "TI"]], 
            UnderoverscriptBox["\[Sum]", 
             RowBox[{
               StyleBox["i", "TI"], "\[LongEqual]", "1"}], 
             StyleBox["n", "TI"], LimitsPositioning -> False], 
            SubscriptBox[
             StyleBox["y", "TI"], 
             StyleBox["i", "TI"]]}], 
          RowBox[{
            StyleBox["n", "TI"], 
            UnderoverscriptBox["\[Sum]", 
             RowBox[{
               StyleBox["i", "TI"], "\[LongEqual]", "1"}], 
             StyleBox["n", "TI"], LimitsPositioning -> False], 
            SubsuperscriptBox[
             StyleBox["x", "TI"], 
             StyleBox["i", "TI"], "2"], "-", 
            SuperscriptBox[
             RowBox[{"(", 
               RowBox[{
                 UnderoverscriptBox["\[Sum]", 
                  RowBox[{
                    StyleBox["i", "TI"], "\[LongEqual]", "1"}], 
                  StyleBox["n", "TI"], LimitsPositioning -> False], 
                 SubscriptBox[
                  StyleBox["x", "TI"], 
                  StyleBox["i", "TI"]]}], ")"}], "2"]}]]}], TraditionalForm], 
     "errors" -> {}, "input" -> 
     "\\beta=\\frac{n \\sum\\limits_{i=1}^{n} x_i y_i - \
\\sum\\limits_{i=1}^{n} x_i \\sum\\limits_{i=1}^{n} y_i}{n \
\\sum\\limits_{i=1}^{n} x_i^2 - \\left(\\sum\\limits_{i=1}^{n} \
x_i\\right)^2}", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "eb703f87-3a81-4b1e-a169-422093e3031b"]
}], "Text",
